{
    "feature_args": {
        "-input": {
            "dest": "input_path",
            "help": "Input data path",
            "required": "True"
        },
        "-output": {
            "dest": "output_path",
            "help": "Output data path",
            "required": "True"
        },
        "-mapping": {
            "dest": "mapping_path",
            "help": "Mapping data path",
            "required": "True"
        },
        "-run_date": {
            "dest": "run_date",
            "help": "As-of date of running process",
            "required": "True"
        },
        "-description": {
            "dest": "description",
            "help": "Process description",
            "required": "True"
        }
    },
    "extraction": {
        "description": "Serge Feldman",
        "input": {
            "read": {
                "file_type": "csv",
                "separator": "|",
                "apply_dtype": {
                    "INS_CODE": "str",
                    "INS_OPTION_MNEMO": "str",
                    "SERVICE_PROVIDED": "str",
                    "EOY_AMT": "float",
                    "CLIENT_TYPE": "str"
                }
            },
            "plugin": null
        },
        "mapping": {
            "read": {
                "file_type": "excel",
                "skip_rows": 0,
                "use_cols": "A,B",
                "sheet_name": 0,
                "apply_dtype": {
                    "INS": "str",
                    "INS_ACCOUNT": "str"
                }
            },
            "left_on": [
                "INS_CODE"
            ],
            "right_on": [
                "INS"
            ],
            "plugin": null
        },
        "assign": {
            "col_const": {
                "ASSIGN_CURRENCY": "USD",
                "ASSIGN_ACCOUNT": "SFELDMAN",
                "ASSIGN_COMMENTS": ""
            },
            "col_var": {
                "ASSIGN_DATE": "run_date",
                "ASSIGN_DESCRIPTION": "description"
            },
            "plugin": null
        },
        "output": {
            "rearrange": {
                "col_rename": {
                    "EOY_AMT": "AMOUNT",
                    "INS_OPTION_MNEMO": "EXT_ACCOUNT",
                    "INS_ACCOUNT": "MAP_ACCOUNT",
                    "ASSIGN_CURRENCY": "CURRENCY",
                    "ASSIGN_ACCOUNT": "ACCOUNT",
                    "ASSIGN_DATE": "RUN_DATE",
                    "ASSIGN_DESCRIPTION": "DESCRIPTION",
                    "ASSIGN_COMMENTS": "COMMENTS",
                    "CLIENT_TYPE": "TYPE"
                },
                "col_reorder": [
                    "RUN_DATE", "DESCRIPTION", "AMOUNT", "CURRENCY", "ACCOUNT", "EXT_ACCOUNT", "MAP_ACCOUNT", "TYPE", "COMMENTS"
                ],
                "plugin": null
            },
            "write": {
                "file_type": "csv",
                "separator": "|"
            },
            "plugin": "fall2021.apps.flowdata.src.plugin_util.add_datetime_into_comments_plugin"
        }
    }
}